@ARTICLE{Duek2014-zw,
  title    = "Reward sensitivity and anger in euthymic bipolar disorder",
  author   = "Duek, Or and Osher, Yamima and Belmaker, Robert H and Bersudsky,
              Yuly and Kofman, Ora",
  abstract = "According to the hypersensitive behavioral approach system (BAS)
